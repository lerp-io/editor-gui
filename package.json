{
  "name": "editor-gui",
  "version": "0.1.2",
  "description": "basic react web ui components",
  "main": "./components/index.coffee",
  "scripts": {
    "dev": "webpack-dev-server --config ./scripts/demo.config.js --mode development",
    "dev-docs": "coffee -c -b -w --output ./examples/ ./examples/*.coffee & webpack-dev-server --config ./scripts/docs.config.js --mode development",
    "docs": "coffee -c -b --output ./examples/ ./examples/*.coffee & webpack --config ./scripts/docs.config.js --mode production",
    "examples": "coffee -c -b --output ./examples/ ./examples/*.coffee",
    "prod": "NODE_ENV=production webpack --config ./scripts/lib.config.js --progress --mode development && NODE_ENV=production webpack --config ./scripts/lib.config.js --progress --mode development && webpack --config ./scripts/demo.config.js --progress --mode production",
    "lib": "coffee -cmw --output ./dist/ ./components & webpack --config ./scripts/lib.config.js --watch --progress --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lerp-io/re-lui.git"
  },
  "homepage": "https://lerp-io.github.io/re-lui/",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "classnames": "^2.2.6",
    "coffee-loader": "^1.0.0",
    "coffeescript": "^2.5.1",
    "color": "^3.1.0",
    "css": "^2.2.4",
    "css-loader": "^3.5.3",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize.css": "^8.0.1",
    "raw-loader": "^4.0.1",
    "re-slide": "^1.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-virtualized": "^9.21.2",
    "remark-gfm": "^1.0.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "color": "^3.1.2",
    "css": "^2.2.4",
    "re-slide": "^1.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "highlight.js": "^10.4.1",
    "parse-color": "^1.0.0",
    "react-markdown": "^5.0.3",
    "string-replace-loader": "^3.0.1",
    "ts-loader": "^8.0.11"
  }
}
