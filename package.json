{
  "name": "editor-gui",
  "version": "0.2.2",
  "description": "basic react web ui components",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "coffee -c -b -w --output ./examples/ ./examples/*.coffee & NODE_ENV=development webpack serve --config ./scripts/docs.config.js --mode development",
    "prod": "coffee -cmw --output ./dist/ ./components & coffee -c -b --output ./examples/ ./examples/*.coffee && NODE_ENV=production webpack --config ./scripts/lib.config.js --progress --mode development && NODE_ENV=production webpack --config ./scripts/lib.config.js --progress --mode development && webpack --config ./scripts/docs.config.js --progress --mode production",
    "lib": "coffee -cmw --output ./dist/ ./components & webpack --config ./scripts/lib.config.js --watch --progress --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lerp-io/re-lui.git"
  },
  "homepage": "https://lerp-io.github.io/re-lui/",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-react": "^7.12.13",
    "@mdx-js/loader": "^1.6.22",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.6",
    "coffee-loader": "^2.0.0",
    "coffeescript": "^2.5.1",
    "color": "^3.1.0",
    "css": "^3.0.0",
    "css-loader": "^5.0.1",
    "highlight.js": "^10.5.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.5",
    "normalize.css": "^8.0.1",
    "parse-color": "^1.0.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-virtualized": "^9.22.3",
    "remark-gfm": "^1.0.0",
    "string-replace-loader": "^3.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.15",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "color": "^3.1.3",
    "css": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "@babel/preset-env": "^7.12.13"
  }
}
